{
  "name": "@politico/interactive-templates",
  "version": "0.0.8",
  "description": "Templating engine for generating codebases.",
  "main": "dist/scripts/index.js",
  "bin": {
    "pit": "dist/cli.js"
  },
  "scripts": {
    "dev": "nodemon --ignore dist --exec npm run build",
    "build": "babel src --out-dir dist --copy-files",
    "test": "nyc mocha $(find src/**/**/test.js -name '*.js') --require \"@babel/register\" --timeout 30000",
    "test-specific": "nyc mocha $(find src/**/**/test.js -name '*.js') --require \"@babel/register\" --timeout 30000 -g",
    "posttest": "rmdir example"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@politico/eslint-config-interactives": "^0.0.4",
    "@politico/interactive-bin": "^0.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.14.1",
    "eslint-plugin-mocha": "^5.3.0",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.1.0",
    "yarn": "^1.9.4"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.2",
    "child-process-promise": "^2.2.1",
    "ejs": "^2.6.1",
    "figlet": "^1.2.1",
    "fs-extra": "^7.0.1",
    "glob-gitignore": "^1.0.13",
    "inquirer": "^6.2.2",
    "lodash": "^4.17.11",
    "npm-api": "^0.4.12",
    "rimraf": "^2.6.3",
    "yargs": "^12.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:The-Politico/politico-interactive-templates.git"
  }
}
